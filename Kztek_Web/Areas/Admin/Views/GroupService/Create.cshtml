
@model Kztek_Model.Models.Service_Submit

@await Html.PartialAsync("Partial_Action", new Kztek_Library.Models.PartialActionModel<Kztek_Model.Models.Service_Submit>()
{
    Model = Model,
    Controller = "GroupService",
    Action = "Create",
    Page = 1,
    

})
@section scripts{
   
    <script>
        $(document).ready(function () {
            $("#Countinue").click(function () {
                $("#SaveAndCountinue").val('true');
            });

            $("#BackToList").click(function () {
                $("#SaveAndCountinue").val('false');
            });

            $("#frmGroupservice").submit(function () {
                var lstId = "";

                $("input[name='chkCheckBox']").each(function () {
                    var cmd = $(this);
                    var id = cmd.prop("value");

                    if ($(this).is(":checked")) {
                        lstId = lstId + id + ";";
                    }
                });

                $("#Groupids").val(lstId);
                var s1 = $("#Groupids").val();
                console.log(s1);
               
            });
        });
    </script>
}
