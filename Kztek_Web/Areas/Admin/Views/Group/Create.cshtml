
@model Kztek_Model.Models.Group

@await Html.PartialAsync("Partial_Action", new Kztek_Library.Models.PartialActionModel<Kztek_Model.Models.Group>()
{
    Model = Model,
    Controller = "Group",
    Action = "Create",
    Page = 1,
    Data_Select_1 = (Kztek_Library.Models.SelectListModel_Chosen)ViewBag.camera_LPR_1,
    Data_Select_2 = (Kztek_Library.Models.SelectListModel_Chosen)ViewBag.camera_LPR_2,
    Data_Select_3 = (Kztek_Library.Models.SelectListModel_Chosen)ViewBag.camera_Panorama_1,
    Data_Select_4 = (Kztek_Library.Models.SelectListModel_Chosen)ViewBag.camera_Panorama_2

})
@section scripts{
    <script src="~/modules/Parking/tblLaneController.js"></script>
    <script>
        $(document).ready(function () {
            $("#Countinue").click(function () {
                $("#SaveAndCountinue").val('true');
            });

            $("#BackToList").click(function () {
                $("#SaveAndCountinue").val('false');
            });
        });
    </script>
}
